<% form_for(@sale_order_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :sale_order_id %><br />
    <%= f.text_field :sale_order_id %>
  </p>
  <p>
    <%= f.label :purchase_order_item_id %><br />
    <%= f.text_field :purchase_order_item_id %>
  </p>
  <p>
    <%= f.label :unit_price %><br />
    <%= f.text_field :unit_price %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
